# API Configuration
VITE_API_BASE_URL=https://your-api-url.com/api

# Authentication Endpoints
VITE_API_AUTH_LOGIN=https://your-api-url.com/api/auth/login
VITE_API_AUTH_REGISTER=https://your-api-url.com/api/auth/user/register

# Application Config
VITE_APP_NAME=Zyntra
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# Storage Keys
VITE_STORAGE_AUTH_TOKEN=auth_token
VITE_STORAGE_TOKEN_TYPE=token_type
VITE_STORAGE_USER_TYPE=user_type
VITE_STORAGE_USER_DATA=user_data

# Feature Flags
VITE_ENABLE_3D_EFFECTS=false
VITE_ENABLE_ANIMATIONS=true
